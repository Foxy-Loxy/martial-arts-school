    "class-validator": "^0.12.1",
    "express": "^4.17.3",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "dotenv": "^8.2.0",
